#Config
TCPKeepAlive=yes
ServerAliveInterval=15
ServerAliveCountMax=6
StrictHostKeyChecking=no
Compression=yes
ForwardAgent yes

SendEnv USERID
SendEnv USERPASSWORD
SendEnv AWS_ACCESS_KEY_ID
SendEnv AWS_SECRET_ACCESS_KEY

IdentityFile ~/.ssh/id_rsa_work

Host *
    User ubuntu

Host github
    Hostname github.com
    IdentityFile ~/.ssh/id_rsa
    User ashokrajar

Host codengine
    ForwardAgent yes
    Hostname 192.168.10.254
    IdentityFile ~/.ssh/id_rsa
    User ashokrajar

Host sandstone
    ForwardAgent yes
    Hostname 192.168.1.110
    User root
    Port 2222

Host snowhite
    ForwardAgent yes
    Hostname 192.168.1.120
    User root
    Port 2222

Host ubudroid
    ForwardAgent yes
    Hostname 192.168.1.120
    User ashokraja
    Port 22

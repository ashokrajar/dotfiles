# Include local configs
Include localconfig

#Config
TCPKeepAlive yes
ServerAliveInterval 20
ServerAliveCountMax 5
StrictHostKeyChecking no
UserKnownHostsFile /dev/null
Compression no
ForwardAgent yes
ForwardX11 no
AddKeysToAgent yes
StrictHostKeyChecking yes

SendEnv USERID
SendEnv USERPASSWORD
SendEnv AWS_ACCESS_KEY_ID
SendEnv AWS_SECRET_ACCESS_KEY

IdentityFile ~/.ssh/id_ecdsa

Host github.com
    User ashokrajar

Host bitbucket.org
    User ashokrajar

Host atom.ashokraja.in
    User pi

Host *.ashokraja.in
    User ashokraja

Host blog
    Hostname mylife.ashokraja.in
    User ubuntu

Host remoteproxy\
    Hostname us-proxy.ashokraja.in
    User ubuntu
